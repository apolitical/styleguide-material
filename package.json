{
  "name": "@apolitical/styleguide-material",
  "version": "2.0.3-rc.1",
  "description": "Collection of components for Apolitical built on material-ui",
  "main": "build/index.js",
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "npx styleguidist server",
    "build": "NODE_ENV=production babel src -d build",
    "dev-lite": "concurrently --names 'SRVR,BUILD'  -c 'black.bgCyan.bold,black.bgWhite.bold' 'npm run start' 'npm run start:build'",
    "start:build": "nodemon --exec 'npm run build || true'",
    "build:styleguide": "npx styleguidist build",
    "test": "jest",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "flow": "flow",
    "lint": "eslint src",
    "preversion": "npm t && npm run lint && npm run flow",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apolitical/styleguide-material.git"
  },
  "engines": {
    "node": "^10.5.0",
    "npm": "^6.1.0"
  },
  "author": "Thomas Geraghty <thomas.geraghty@apolitical.co>",
  "license": "MIT",
  "files": [
    "build/**",
    "CHANGELOG.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "LICENSE",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/apolitical/styleguide-material/issues"
  },
  "homepage": "https://github.com/apolitical/styleguide-material#readme",
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "plugin:flowtype/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "flowtype",
      "flow-header",
      "jsx-a11y",
      "react"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "flow-header/flow-header": "error",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/setupTests.js",
            "**/*test.js*"
          ]
        }
      ],
      "no-restricted-properties": [
        "error",
        {
          "object": "require",
          "property": "ensure",
          "message": "Please use import() instead. More info: https://facebook.github.io/create-react-app/docs/code-splitting"
        },
        {
          "object": "System",
          "property": "import",
          "message": "Please use import() instead. More info: https://facebook.github.io/create-react-app/docs/code-splitting"
        }
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ],
      "@babel/flow",
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      "@babel/plugin-proposal-class-properties",
      "add-module-exports"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "roots": [
      "src"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "concurrently": "^4.1.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flow-header": "^0.2.0",
    "flow-bin": "^0.87.0",
    "nodemon": "^1.18.7",
    "react-styleguidist": "^8.0.4"
  }
}
